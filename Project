import geopandas as gpd

# Download LSOA boundaries (generalised for faster processing)
url = "https://opendata.arcgis.com/datasets/1f23484eafea45f98485ef816e4fee2d_0.geojson"
lsoa = gpd.read_file(url)

# Save to your project folder
lsoa.to_file("lsoa_boundaries.geojson", driver="GeoJSON")
print("File saved as 'lsoa_boundaries.geojson'")
